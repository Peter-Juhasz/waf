            "tags": [ "JavaScript", "NPM" ]
        },
        {
            "term": "package-lock.json",
            "category": "Source Code Disclosure",
            "locations": [ "PathFileName" ],
            "tags": [ "JavaScript", "NPM" ]
        },
        {
            "term": "npm-shrinkwrap.json",
            "category": "Source Code Disclosure",
            "locations": [ "PathFileName" ],
            "tags": [ "JavaScript", "NPM" ]
        },
        {
